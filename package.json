{
  "name": "bug-whisperer",
  "version": "1.0.0",
  "description": "Bug Whisperer MVP - JavaScript code analyzer with ESLint integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "echo 'Use npm run build for production deployment'",
    "build": "cd frontend && npm run build",
    "postinstall": "cd frontend && npm install",
    "deploy:railway": "railway up",
    "deploy:heroku": "git push heroku main"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "eslint": "^8.57.0",
    "dotenv": "^16.3.1",
    "@octokit/rest": "^20.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": ["eslint", "code-analysis", "bug-detection"],
  "author": "Bug Whisperer Team",
  "license": "MIT"
}